<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net8.0</TargetFramework>
    <RootNamespace>FileMatch</RootNamespace>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>warnings</Nullable>
    <AssemblyName>fmatch</AssemblyName>
    <ProduceReferenceAssembly>False</ProduceReferenceAssembly>
    <PackageId>FileMatch</PackageId>
    <Title>File Match</Title>
    <Authors>Arturo Torres</Authors>
    <Description>Small application to find duplicate files on directories. </Description>
    <Copyright>Copyright ©2023 ArturoTorres</Copyright>
    <PackageProjectUrl>https://github.com/ArtTorres/FileMatch</PackageProjectUrl>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="MagnetArgs" Version="0.6.2.13" />
  </ItemGroup>

</Project>
